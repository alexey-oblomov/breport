  "scripts": {
    "start": "node config/webpack-dev-server.js",
    "build": "`npm bin`/webpack --config ./config/webpack.config.prod.js --mode production",
    "lint": "`npm bin`/eslint --ext .js,.jsx --config ./.eslintrc.js ./src",
    "lintfix": "`npm bin`/eslint --fix --ext .js,.jsx --config ./.eslintrc.js ./src",
    "build-stats": "webpack --config ./config/webpack.config.prod.js --mode production --profile --json > stats.json",
    "analyze": "npm run build-stats && cp ./stats.json ./dist/stats.json && webpack-bundle-analyzer ./dist/stats.json",
    "heroku-postbuild": "`npm bin`/webpack --config ./config/webpack.config.prod.js --mode production"
  },
