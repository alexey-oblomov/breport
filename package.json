{
  "name": "breport-spa",
  "version": "1.0.0",
  "private": true,
  "description": "Battle Report tool for Eve Online",
  "main": "index.jsx",
  "scripts": {
    "start": "node config/webpack-dev-server.js",
    "build": "webpack --config ./config/webpack.config.prod.js --mode production",
    "lint": "eslint --ext .js,.jsx --config ./.eslintrc.js ./src",
    "lint:fix": "eslint --fix --ext .js,.jsx --config ./.eslintrc.js ./src",
    "lint:staged": "eslint --fix --ext .js,.jsx --config ./.eslintrc.js",
    "lint:commit": "`npm bin`/lint-staged",
    "build-stats": "webpack --config ./config/webpack.config.prod.js --mode production --profile --json > ./dist/stats.json",
    "analyze": "npm run build-stats && webpack-bundle-analyzer ./dist/stats.json",
    "heroku-postbuild": "npm run build"
  },
  "keywords": [],
  "author": "maullerz",
  "license": "MIT",
  "engines": {
    "node": "10.x",
    "npm": "6.7.x"
  },
  "browserslist": [
    "defaults",
    "not IE 11",
    "not IE_Mob 11",
    "maintained node versions"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.(js|jsx)": [
      "npm run lint:staged",
      "git add"
    ]
  },
